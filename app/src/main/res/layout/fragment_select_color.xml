                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor036_MidnightBlue"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor049_LightCyan"
                    app:layout_constraintStart_toEndOf="@+id/hexColor041_DodgerBlue"
