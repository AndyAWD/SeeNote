                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/hexColor045_SkyBlue"
                    app:layout_constraintTop_toTopOf="@+id/hexColor036_MidnightBlue" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor061_DarkGreen"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor061_DarkGreen"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    android:text=""
                    android:textSize="@{viewModel.size}"
