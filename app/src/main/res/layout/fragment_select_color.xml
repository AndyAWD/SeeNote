                    app:layout_constraintEnd_toStartOf="@+id/hexColor063_ForestGreen"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/hexColor036_MidnightBlue" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor063_ForestGreen"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor063_ForestGreen"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor061_DarkGreen"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor076_LimeGreen"
                    app:layout_constraintStart_toEndOf="@+id/hexColor061_DarkGreen"
                    app:layout_constraintTop_toTopOf="@+id/hexColor061_DarkGreen" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor076_LimeGreen"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor076_LimeGreen"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor061_DarkGreen"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor069_LightGreen"
                    app:layout_constraintStart_toEndOf="@+id/hexColor063_ForestGreen"
                    app:layout_constraintTop_toTopOf="@+id/hexColor061_DarkGreen" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor069_LightGreen"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor069_LightGreen"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor061_DarkGreen"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor074_GreenYellow"
                    app:layout_constraintStart_toEndOf="@+id/hexColor076_LimeGreen"
                    app:layout_constraintTop_toTopOf="@+id/hexColor061_DarkGreen" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor074_GreenYellow"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor074_GreenYellow"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor061_DarkGreen"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/hexColor069_LightGreen"
                    app:layout_constraintTop_toTopOf="@+id/hexColor061_DarkGreen" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor102_Maroon"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor102_Maroon"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    android:text=""
                    android:textSize="@{viewModel.size}"
                    app:layout_constraintBottom_toTopOf="@+id/hexColor101_SaddleBrown"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor105_Firebrick"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/hexColor061_DarkGreen" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor105_Firebrick"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor105_Firebrick"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor102_Maroon"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor115_Red"
                    app:layout_constraintStart_toEndOf="@+id/hexColor102_Maroon"
                    app:layout_constraintTop_toTopOf="@+id/hexColor102_Maroon" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor115_Red"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor115_Red"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor102_Maroon"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor111_LightSalmon"
                    app:layout_constraintStart_toEndOf="@+id/hexColor105_Firebrick"
                    app:layout_constraintTop_toTopOf="@+id/hexColor102_Maroon" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor111_LightSalmon"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor111_LightSalmon"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor102_Maroon"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor121_Pink"
                    app:layout_constraintStart_toEndOf="@+id/hexColor115_Red"
                    app:layout_constraintTop_toTopOf="@+id/hexColor102_Maroon" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor121_Pink"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor121_Pink"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor102_Maroon"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/hexColor111_LightSalmon"
                    app:layout_constraintTop_toTopOf="@+id/hexColor102_Maroon" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor101_SaddleBrown"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor101_SaddleBrown"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    android:text=""
                    android:textSize="@{viewModel.size}"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor099_Chocolate"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/hexColor102_Maroon" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor099_Chocolate"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
