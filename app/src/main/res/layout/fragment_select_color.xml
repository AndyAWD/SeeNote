<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>

        <variable
            name="viewModel"
            type="tw.com.andyawd.seenote.selectcolor.SelectColorViewModel" />
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.google.android.material.appbar.MaterialToolbar
            android:id="@+id/fscMtToolBar"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:navigationIcon="@drawable/ic_baseline_arrow_back_ios_24" />

        <androidx.core.widget.NestedScrollView
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:fillViewport="true"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/fscMtToolBar">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <com.google.android.material.textview.MaterialTextView
                    android:id="@+id/fscMtvTextSize"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:text="@{@string/setting_note_text_size(viewModel.size)}"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0"
                    app:layout_constraintVertical_chainStyle="packed"
                    tools:text="@string/setting_note_text_size" />

                <androidx.appcompat.widget.AppCompatSeekBar
                    android:id="@+id/fscAcsbTextSize"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="16dp"
                    android:layout_marginTop="@dimen/magic_8dp"
                    android:layout_marginEnd="16dp"
                    android:max="400"
                    android:padding="@dimen/magic_8dp"
                    android:thumb="@drawable/square_b22222_size40"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/fscMtvTextSize"
                    app:layout_constraintVertical_bias="0"
                    app:layout_constraintVertical_chainStyle="packed" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor000_Black"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor000_Black"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    android:text=""
                    android:textSize="@{viewModel.size}"
                    app:layout_constraintBottom_toTopOf="@+id/hexColor036_MidnightBlue"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor002_Gray"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/fscAcsbTextSize"
                    app:layout_constraintVertical_bias="0.0"
                    app:layout_constraintVertical_chainStyle="packed" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor002_Gray"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor002_Gray"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor000_Black"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor004_Silver"
                    app:layout_constraintStart_toEndOf="@+id/hexColor000_Black"
                    app:layout_constraintTop_toTopOf="@+id/hexColor000_Black" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor004_Silver"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor004_Silver"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor000_Black"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor006_Gainsboro"
                    app:layout_constraintStart_toEndOf="@+id/hexColor002_Gray"
                    app:layout_constraintTop_toTopOf="@+id/hexColor000_Black" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor006_Gainsboro"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor006_Gainsboro"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor000_Black"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor007_White"
                    app:layout_constraintStart_toEndOf="@+id/hexColor004_Silver"
                    app:layout_constraintTop_toTopOf="@+id/hexColor000_Black" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor007_White"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor007_White"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor000_Black"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/hexColor006_Gainsboro"
                    app:layout_constraintTop_toTopOf="@+id/hexColor000_Black" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor036_MidnightBlue"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_margin="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor036_MidnightBlue"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    android:text=""
                    android:textSize="@{viewModel.size}"
                    app:layout_constraintBottom_toTopOf="@+id/hexColor061_DarkGreen"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor039_MediumBlue"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/hexColor000_Black" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor039_MediumBlue"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor039_MediumBlue"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor036_MidnightBlue"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor041_DodgerBlue"
                    app:layout_constraintStart_toEndOf="@+id/hexColor036_MidnightBlue"
                    app:layout_constraintTop_toTopOf="@+id/hexColor036_MidnightBlue" />

                <com.google.android.material.button.MaterialButton
                    android:id="@+id/hexColor041_DodgerBlue"
                    android:layout_width="0dp"
                    android:layout_height="0dp"
                    android:layout_marginStart="@dimen/magic_4dp"
                    android:layout_marginEnd="@dimen/magic_4dp"
                    android:backgroundTint="@color/hexColor041_DodgerBlue"
                    android:insetTop="0dp"
                    android:insetBottom="0dp"
                    app:layout_constraintBottom_toBottomOf="@+id/hexColor036_MidnightBlue"
                    app:layout_constraintEnd_toStartOf="@+id/hexColor045_SkyBlue"
                    app:layout_constraintStart_toEndOf="@+id/hexColor039_MediumBlue"
                    app:layout_constraintTop_toTopOf="@+id/hexColor036_MidnightBlue" />

                <com.google.android.material.button.MaterialButton
